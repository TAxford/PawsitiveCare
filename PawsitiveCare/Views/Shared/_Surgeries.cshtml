@model IEnumerable<PawsitiveCare.Surgery>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SurgeryDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SurgeryDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pet.PetName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SurgeryDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SurgeryDescription)
        </td>
        <td>
            @Html.DisplayFor(model => item.Pet.PetName)
        </td>
    </tr> 
}

</table>
